{
  "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "root": true,
  "env": {
    "browser": true,
    "amd": true,
    "node": true
  },
  "rules": {
    "quotes": [
      1,
      "single",
      "avoid-escape"
    ]
  }
}
